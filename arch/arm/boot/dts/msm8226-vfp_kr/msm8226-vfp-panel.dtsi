&mdss_dsi0 {
	qcom,mdss_dsi@fd922800 {
		qcom,dsi-pref-prim-pan = <&dsi_tovis_hvga_cmd>;
	};
};

&dsi_tovis_hvga_cmd {
	qcom,cont-splash-enabled;
	qcom,mdss-dsi-te-check-enable;
	qcom,mdss-dsi-te-using-te-pin;
	status = "ok";
};

&soc {
	mdss_dsi0: qcom,mdss_dsi@fd922800 {
		vddio-supply=<&pm8226_lvs1>;
		qcom,platform-te-gpio = <&msmgpio 24 0>;
	};

	i2c@f9925000 { /* BLSP1 QUP3 */
		lm3530@38 {
			compatible = "backlight,lm3530";
			status = "ok";
			reg = <0x38>;
			lm3530,lcd_bl_en = <&msmgpio 60 0x00>;
			lm3530,max_current = <0x37>;
			lm3530,min_brightness = <0>;
			lm3530,default_brightness = <110>;
			lm3530,max_brightness = <255>;
			lm3530,enable_pwm = <0>;
			lm3530,blmap_size = <256>;
			lm3530,blmap = <
