&soc {
	i2c@f9927000 { /* BLSP1 QUP5 */
		atmel_t641@4a {									/*touch ic I2C bus adddress*/
			revision = "rev_a...rev_c";						/*board version */
			compatible = "atmel,t641";						/*dtsi name*/
			status = "ok";								/*OK-used*/
			reg = <0x4a>;								/*touch ic I2C bus adddress 0x4a -> @4a*/
			atmel,fw_name = "atmel/E7_0_53.fw";
		};

		atmel_t641_revD@4a {								/*touch ic I2C bus adddress*/
			revision = "rev_d...";							/*board version */
			compatible = "atmel,t641";						/*dtsi name*/
			status = "ok";								/*OK-used*/
			reg = <0x4a>;								/*touch ic I2C bus adddress 0x4a -> @4a*/
			atmel,fw_name = "atmel/E7_0_65.fw";
		};

	};
};
